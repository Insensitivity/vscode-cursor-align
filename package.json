{
  "name": "cursor-align",
  "displayName": "Cursor Align",
  "description": "Aligns all cursors using spaces.",
  "version": "1.0.3",
  "publisher": "yo1dog",
  "engines": {
    "vscode": "^1.15.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:yo1dog.cursor-align.alignCursors"
  ],
  "main": "./extension/extension.js",
  "contributes": {
    "commands": [{
      "command": "yo1dog.cursor-align.alignCursors",
      "title": "Align Cursors"
    }],
    "keybindings": [{
      "command": "yo1dog.cursor-align.alignCursors",
      "key": "alt+a",
      "when": "editorTextFocus"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "mocha": "^3.5.0",
    "vscode": "^1.1.5"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/yo1dog/vscode-cursor-align/issues",
    "email": "mike@yo1.dog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yo1dog/vscode-cursor-align.git"
  },
  "homepage": "https://github.com/yo1dog/vscode-cursor-align"
}
